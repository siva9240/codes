package package_45;
public class sample45{
public static void main(String[]args){
    String a="Aja";
    String b="";
    for(int i=0;i<a.length();i++){
    char t=a.charAt(i);
    b=t+b;
    }
    if(b.equalsIgnoreCase(a)){
    System.out.println(b);
    }
    else{
    System.out.print("it's not");
    
    }
    
    }
}
