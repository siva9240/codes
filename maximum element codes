package package_2;
public class arrayleftrotate{
    public static  void main(String[]args){
       int []arr={23,54,65,56};
       int max=arr[0];
       for(int i=1;i<arr.length;i++){
         if(arr[i]>max){
             max=arr[i];
         }
       }
       {
           System.out.println("maximum element is :"+max);
       }
         }
         
       }
