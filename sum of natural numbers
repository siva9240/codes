package com.java;
import java.util.Scanner;
class Sample2{
public static void main(String[]args){
    Scanner Scan=new Scanner(System.in);
    
    int a=Scan.nextInt();
    int Sum=0;
    for(int i=0;i<=0;i++){
        if(i%2==0){
            Sum=Sum+1;
        }
        System.out.println(Sum);
    }
    }
}
